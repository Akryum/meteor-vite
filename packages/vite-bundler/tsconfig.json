{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "typeRoots": [
      "../../examples/vue/node_modules/@types",
      "../../examples/vue/node_modules",
      "../../npm-packages"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "paths": {
      "meteor/*": [
        "../../examples/vue/node_modules/@types/meteor/*",
        "../../examples/vue/.meteor/local/types/packages.d.ts"
      ]
    },
    "skipLibCheck": true
  }
}
